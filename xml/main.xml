	<!-- CAMERA SETTINGS KEY
	
	setting id:
	0 = mode
	1 = OIS
	2 = flash
	3 = wide angle
	4 = auto red-eye reduction
	5 = zoom
	6 = spot metering
	7 = F
	8 = speed
	9 = backlight compensation
	
	modeID:
	0 = portrait mode
	1 = simple mode
	2 = macro mode
	3 = high sensitivity mode
	4 = night portrait mode
	5 = beach mode
	6 = sports mode
	7 = manual mode
	8 = scenery mode
	
	state:
	0 = "OFF"
	1 = "ON"
	
	zoomID:
	0 = zoom
	1 = optical zoom
	2 = extra optical zoom
	
